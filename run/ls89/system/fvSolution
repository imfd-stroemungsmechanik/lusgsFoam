FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(omega|k|gammaInt|ReThetat)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-12;
        relTol          0.01;
    };
}

LUSGS
{
    omega   1.0;
    nIter   2;
}

