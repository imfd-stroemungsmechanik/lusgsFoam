FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     lusgsFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         4e-03;

deltaT          1e-7;

writeControl    runTime;

writeInterval   1e-03;

purgeWrite      0;

writeFormat     ascii;

writePrecision  15;

writeCompression off;

timeFormat      general;

timePrecision   6;

